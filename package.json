{
  "$schema": "https://fbflipper.com/schemas/plugin-package/v2.json",
  "name": "flipper-plugin-rn-network-mock",
  "id": "rn-network-mock",
  "version": "1.0.3",
  "pluginType": "client",
  "main": "dist/bundle.js",
  "flipperBundlerEntry": "src/index.tsx",
  "license": "MIT",
  "repository": "https://github.com/nilavanraj/flipper-plugin-rn-network-mock",
  "author": "Nilavan Raj <nilavanraj1@gmail.com> (https://github.com/nilavanraj)",
  "bugs": {
    "url": "https://github.com/nilavanraj/flipper-plugin-rn-network-mock/issues"
  },
  "homepage": "https://github.com/nilavanraj/flipper-plugin-rn-network-mock#readme",
  "keywords": [
    "flipper-plugin"
  ],
  "icon": "network",
