    "@types/jest": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "antd": "latest",
    "flipper-pkg": "latest",
    "flipper-plugin": "latest",
    "jest": "latest",
    "jest-mock-console": "latest",
    "react": "latest",
    "react-dom": "latest",
    "typescript": "latest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "dependencies": {
    "install": "^0.13.0",
    "npm": "^10.2.5",
    "react-json-editor-ajrm": "^2.5.14",
    "react-json-view": "^1.21.3"
